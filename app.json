{
  "expo": {
    "name": "SimRun",
    "slug": "SimRun",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sim-run-scheme",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "locales": {
      "en": "./locales/en.json",
      "zh": "./locales/zh.json"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "UIBackgroundModes": ["location", "fetch", "processing"],
        "ITSAppUsesNonExemptEncryption": false,
        "NSSupportsLiveActivities": true,
        "BGTaskSchedulerPermittedIdentifiers": [
          "background-fetch-task",
          "SIM_RUN_RUNNING_TRACKER_TASK",
          "expo-background-fetch"
        ]
      },
      "appleTeamId": "3NYLR6AY46",
      "config": {
        "googleMapsApiKey": ""
      },
      "entitlements": {
        "com.apple.security.application-groups": ["group.com.shetepro.simrun"],
        "com.apple.developer.group-session": true,
        "aps-environment": "production",
        "com.apple.developer.icloud-container-identifiers": [
          "iCloud.com.shetepro.simrun"
        ],
        "com.apple.developer.icloud-services": ["CloudKit"]
      },
      "bundleIdentifier": "com.shetepro.simrun"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "FOREGROUND_SERVICE",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACTIVITY_RECOGNITION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION"
      ],
      "package": "com.anonymous.simrun"
    },
    "updates": {
      "enabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-sqlite",
      [
        "expo-location",
        {
          "isIosBackgroundLocationEnabled": true,
          "isAndroidBackgroundLocationEnabled": true
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-preview.png",
          "imageWidth": 120,
          "resizeMode": "contain",
          "backgroundColor": "#4f46e5",
          "dark": {
            "image": "./assets/images/icon-preview.png",
            "backgroundColor": "#1e1b4b"
          }
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-image-picker",
      "@bacons/apple-targets"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2e9b7289-40c2-40bf-89b1-c2ae8f7dccfc"
      }
    }
  }
}
